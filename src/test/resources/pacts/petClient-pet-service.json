{
    "provider": {
        "name": "pet-service"
    },
    "consumer": {
        "name": "petClient"
    },
    "interactions": [
        {
            "description": "A request to /pets",
            "request": {
                "method": "POST",
                "path": "/pets",
                "headers": {
                    "Content-Type": "application/json"
                },
                "body": {
                    "name": "Bruno",
                    "type": "Dog"
                },
                "matchingRules": {
                    "body": {
                        "$.name": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.type": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                }
            },
            "response": {
                "status": 200
            },
            "providerStates": [
                {
                    "name": "Create a new pet"
                }
            ]
        },
        {
            "description": "A request to /pets",
            "request": {
                "method": "GET",
                "path": "/pets"
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json"
                },
                "body": {
                    "name": "Bruno",
                    "type": "Dog"
                },
                "matchingRules": {
                    "body": {
                        "$.name": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.type": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                }
            },
            "providerStates": [
                {
                    "name": "Pet exists"
                }
            ]
        }
    ],
    "metadata": {
        "pactSpecification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.5.24"
        }
    }
}